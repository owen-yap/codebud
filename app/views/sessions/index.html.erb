<div class="col-8">

    <h2 class="subheading mt-5">Past Questions</h2>

    <% @session.each do |session| %>
        <div class="card-template question mb-2">
          <div class="info">
            <h6 class="title mb-2"><%=session.question.title %> </h6>
            <p> <%= session.question.rich_body %></p>
          </div>
          <div><%= link_to "View",question_messages_path(session.question), class: "btn fat-btn accepted" %></div>
        </div>
    <% end %>

</div>
